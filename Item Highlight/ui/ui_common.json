{
    "cache_toggle_texture": {
        "type": "panel",
        "size": [
            "100%",
            "100%"
        ],
        "$toggle_unactive_texture|default": "textures/ui/button_borderless_light",
        "$toggle_active_texture|default": "textures/ui/button_borderless_lightpressednohover",
        "$hide_outline|default": false,
        "$outline_color|default": [
            0,
            0,
            0
        ],
        "controls": [
            {
                "on": {
                    "type": "image",
                    "size": [
                        "100%",
                        "100%"
                    ],
                    "texture": "$toggle_active_texture",
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "$binding_toggle_name",
                            "source_property_name": "#toggle_state",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "off": {
                    "type": "image",
                    "size": [
                        "100% - 2px",
                        "100% - 2px"
                    ],
                    "texture": "$toggle_unactive_texture",
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "$binding_toggle_name",
                            "source_property_name": "(not #toggle_state)",
                            "target_property_name": "#visible"
                        }
                    ]
                }
            },
            {
                "toggleOutline": {
                    "type": "image",
                    "size": [
                        "100%",
                        "100%"
                    ],
                    "texture": "textures/ui/focus_border_selected",
                    "color": "$outline_color",
                    "ignored": "$hide_outline"
                }
            }
        ]
    },
    "cache_toggle_content": {
        "type": "panel",
        "$binding_toggle_name|default": "toggle",
        "$toggle_control_name": "('asaki_zuki_data:' + $binding_toggle_name)",
        "$output_data_single_name": "('asaki_zuki_toggle:' + $toggle_control_name)",
        "controls": [
            {
                "$output_data_single_name": {
                    "type": "panel",
                    "size": [
                        0,
                        0
                    ],
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "$binding_toggle_name",
                            "resolve_sibling_scope": true,
                            "source_property_name": "(#output_index - ((#output_index > 4) * 8))",
                            "target_property_name": "#output_index"
                        }
                    ]
                }
            },
            {
                "$binding_toggle_name": {
                    "type": "panel",
                    "size": [
                        0,
                        0
                    ],
                    "property_bag": {
                        "#output_index": 0
                    },
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_control_name": "$toggle_control_name",
                            "resolve_sibling_scope": true,
                            "source_property_name": "#output_index",
                            "target_property_name": "#output_index"
                        },
                        {
                            "binding_type": "view",
                            "source_control_name": "$output_data_single_name",
                            "resolve_sibling_scope": true,
                            "source_property_name": "(#output_index = 4)",
                            "target_property_name": "#toggle_state"
                        }
                    ]
                }
            },
            {
                "$toggle_control_name@common.cache_toggle": {
                    "bindings": [
                        {
                            "binding_type": "view",
                            "source_property_name": "((not not #toggle_state) * 1)",
                            "target_property_name": "#checkWhenAdd"
                        },
                        {
                            "binding_type": "view",
                            "source_control_name": "$output_data_single_name",
                            "resolve_sibling_scope": true,
                            "source_property_name": "#output_index",
                            "target_property_name": "#old_data"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(#checkWhenAdd + #old_data)",
                            "target_property_name": "#output_index"
                        },
                        {
                            "binding_type": "view",
                            "source_property_name": "(not (#toggle_state = #toggle_state))",
                            "target_property_name": "#toggle_state"
                        }
                    ]
                }
            }
        ]
    },
    "cache_toggle": {
        "type": "toggle",
        "unchecked_control": "off",
        "unchecked_hover_control": "offHover",
        "layer": 1,
        "sound_name": "random.click",
        "sound_volume": 1.0,
        "sound_pitch": 1.0,
        "button_mappings": [
            {
                "from_button_id": "button.menu_select",
                "to_button_id": "button.menu_select",
                "mapping_type": "pressed"
            },
            {
                "from_button_id": "button.menu_ok",
                "to_button_id": "button.menu_ok",
                "mapping_type": "focused"
            }
        ],
        "property_bag": {
            "#toggle_state": false
        },
        "controls": [
            {
                "off@common.cache_toggle_texture": {
                    "$toggle_unactive_texture": "textures/ui/button_borderless_light",
                    "$toggle_active_texture": "textures/ui/button_borderless_lightpressednohover",
                    "$outline_color": [
                        0,
                        0,
                        0
                    ]
                }
            },
            {
                "offHover@common.cache_toggle_texture": {
                    "$toggle_unactive_texture": "textures/ui/button_borderless_lighthover",
                    "$toggle_active_texture": "textures/ui/button_borderless_lightpressed",
                    "$outline_color": [
                        1,
                        1,
                        1
                    ]
                }
            }
        ]
    },
    "text_edit_box_label": {
        "bindings": "$edit_textbox_label_bindings",
        "$edit_textbox_label_bindings|default": [
            {
                "binding_type": "$text_edit_box_content_binding_type",
                "binding_condition": "$text_edit_box_binding_condition",
                "binding_collection_name": "$text_edit_box_grid_collection_name",
                "binding_name": "$text_edit_box_content_binding_name",
                "binding_name_override": "#item_name"
            },
            {
                "binding_type": "$text_color_binding_type",
                "binding_name": "$text_color_binding_name",
                "binding_name_override": "#color"
            },
            {
                "binding_name": "#newline_refresh"
            }
        ]
    },
    "text_edit_box": {}
}