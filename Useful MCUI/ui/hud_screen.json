    "anim_actionbar_text_background_alpha_out": {
        "destroy_at_end": "hud_actionbar_text",
        "from": 0.5,
        "to": 0,
        "duration": "$duration",
        "$duration|default": 4,
        "variables": [
            {
                "require": "$top_right_actionbar",
                "$duration": 4.5
            }
        ]
    },
    "anim_actionbar_text_alpha_out": {
        "destroy_at_end": "",
        "duration": "$duration",
        "$duration|default": 4,
        "variables": [
            {
                "require": "$top_right_actionbar",
                "$duration": 4.5
            }
        ]
    },
    "anim_actionbar_text_background_size_in": {
        "anim_type": "size",
        "from": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "to": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "easing": "out_quad",
        "duration": "$duration",
        "$duration|default": 7,
        "next": "@hud.anim_actionbar_text_background_size_out"
    },
    "anim_actionbar_text_background_size_out": {
        "anim_type": "size",
        "from": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "to": [
            "100%c + 12px",
            0
        ],
        "duration": 1,
        "easing": "in_out_cubic"
    },
    "hud_actionbar_text": {
        "anchor_from": "$anchor",
        "anchor_to": "$anchor",
        "$anchor|default": "bottom_middle",
        "texture": "textures/ui/hud_tip_text_background",
        "size": "$size",
        "$size|default": [
            "100%c + 12px",
            "100%c + 5px"
        ],
        "offset": "$offset",
        "$offset|default": [
            0,
            -75
        ],
        "alpha": "$alpha",
        "$alpha|default": "@hud.anim_actionbar_text_background_alpha_out",
        "clips_children": "$clip",
        "$clip|default": false,
        "$actionbar": "$actionbar_text",
        "$duration": 4,
        "variables": [
            {
                "requires": "$top_right_actionbar",
                "$anchor": "top_left",
                "$offset": [
                    0,
                    0
                ],
                "$alpha": 0.5,
                "$clip": true,
                "$size": "@hud.anim_actionbar_text_background_size_in"
            },
            {
                "requires": "((not (($actionbar - 'Â§-') = $actionbar)) and $custom_actionbar_command)",
                "$alpha": 0
            },
            {
                "requires": "((not (($actionbar - 'Â§[') = $actionbar)) and $custom_actionbar_command)",
                "$offset": [
                    2.5,
                    0
                ],
                "$anchor": "left_middle"
            },
            {
                "requires": "((not (($actionbar - 'Â§]') = $actionbar)) and $custom_actionbar_command)",
                "$offset": [
                    -2.5,
                    0
                ],
                "$anchor": "right_middle"
            }
        ]
    },
    "hud_actionbar_text/actionbar_message": {
        "size": [
            "default",
            "default"
        ],
        "max_size": "$max_size",
        "$max_size|default": [
            "default",
            "default"
