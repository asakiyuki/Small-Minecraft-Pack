        "$actionbar": "$actionbar_text",
        "text_alignment": "$text_alight",
        "$text_alight|default": "left",
        "shadow": "$shadow",
        "$shadow|default": false,
        "variables": [
            {
                "requires": "$top_right_actionbar",
                "$max_size": [
                    300,
                    "default"
                ]
            },
            {
                "requires": "((not (($actionbar - 'Â§<') = $actionbar)) and $custom_actionbar_command)",
                "$text_alight": "left"
            },
            {
                "requires": "((not (($actionbar - 'Â§=') = $actionbar)) and $custom_actionbar_command)",
                "$text_alight": "center"
            },
            {
                "requires": "((not (($actionbar - 'Â§>') = $actionbar)) and $custom_actionbar_command)",
